
<%= form_with model: @product, data: { turbo: false } do |f|%>
  <%= f.label :name%>
  <%= f.text_field :name%>

  <%= f.label :width%>
  <%= f.number_field :width%>

  <%= f.label :height%>
  <%= f.number_field :height%>

  <%= f.label :weight%>
  <%= f.number_field :weight%>

  <%= f.label :depth%>
  <%= f.number_field :depth%>

  <%= f.label :prod_category_id, 'Categoria'%>
  <%= f.collection_select :prod_category_id, ProdCategory.all, :id, :description%>

  <%= f.label :logo %>
  <%= f.file_field :logo%>

  <%= f.submit %>

<%end%>